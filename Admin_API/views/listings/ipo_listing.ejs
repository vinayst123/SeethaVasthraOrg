        <a href="#" style="text-decoration: none;">Bluestock</a> 
        <a href="#" style="text-decoration: none; color: black;">>IPO</a> 
        <a href="#" style="text-decoration: none; color: black;">>UPCOMING IPO</a>
      </p>
      <h1 class="text-start mb-1">Upcoming IPO</h1>
      <p class="mt-0 mb-4" style="font-size: 80%;">Companies that have filled for an IPO with SEBI. Few details might be disclosed by the companies later.</p>
    </div>
    <div class="hello row row-cols-1 row-cols-md-3">
      
      <!-- Dynamic IPO Cards -->
      <% if (typeof ipoList !== 'undefined' && ipoList.length > 0) { %>
        <% ipoList.slice().reverse().forEach(function(ipo){ %>
          <div class="card shadow-sm p-4 ">
              <!-- Logo and Title -->
              <div class="all-title d-flex align-items-center mb-5 mt-5 ms-1">
                  <img src="<%= ipo.logo %>" alt="<%= ipo.companyName %> Logo" style="max-height: 40px;">
                  <h4 class="text-primary ms-3 mb-0 text-center" style="color: rgb(68, 89, 194) !important;"> 
                    <%= ipo.companyName %>
                  </h4>
              </div>
              <!-- Details Section -->
              <div class="row text-start mb-2 ms-1 mt-3">
                  <div class="col-4">
                      PRICE BAND<br>
                      <strong><%= ipo.priceBand %></strong>
                  </div>
                  <div class="col-4">
                      OPEN<br>
                      <strong><%= new Date(ipo.open).toLocaleDateString('en-GB').replace(/\//g, '-') %></strong>
                    </div>
                  <div class="col-4">
                      CLOSE<br>
                      <strong><%= new Date(ipo.open).toLocaleDateString('en-GB').replace(/\//g, '-') %></strong>
                    </div>
                  <div class="col-4 mt-3">
                      ISSUE SIZE<br>
                      <strong><%= ipo.issueSize %></strong>
                  </div>
                  <div class="col-4 mt-3">
                      ISSUE TYPE<br>
                      <strong><%= ipo.issueType %></strong>
                  </div>
                  <div class="col-4 mt-3">
                      LISTING DATE<br>
                      <strong><%= new Date(ipo.open).toLocaleDateString('en-GB').replace(/\//g, '-') %></strong>
                    </div>
              </div>
              <!-- Buttons -->
              <div class="card-btn d-flex align-items-center justify-content-start twoButton mb-0 mt-2">
                  <a href="<%= ipo.rhpLink %>" class="btn px-4 ms-3 btn-outline-primary" style="background-color: white; color: blue; font-weight: bold;">RHP</a>
                  <a href="<%= ipo.drhpLink %>" class="btn px-4 ms-3 btn-secondary">DRHP</a>
              </div>
          </div>
        <% }); %>
      <% } %>
      
